category TLCore
depend hyphen-base
depend hyph-utf8
shortdesc Norwegian Bokmal and Nynorsk hyphenation patterns.
longdesc Hyphenation patterns for Norwegian Bokmal and Nynorsk in T1/EC and
longdesc UTF-8 encodings.
execute AddHyphen \
	name=bokmal synonyms=norwegian,norsk \
	lefthyphenmin=2 \
	righthyphenmin=2 \
	file=loadhyph-nb.tex \
	file_patterns=hyph-nb.pat.txt \
	file_exceptions=hyph-nb.hyp.txt
execute AddHyphen \
	name=nynorsk \
	lefthyphenmin=2 \
	righthyphenmin=2 \
	file=loadhyph-nn.tex \
	file_patterns=hyph-nn.pat.txt \
	file_exceptions=hyph-nn.hyp.txt
runpattern f tex/generic/hyph-utf8/loadhyph/loadhyph-nb.tex
runpattern f tex/generic/hyph-utf8/loadhyph/loadhyph-nn.tex
runpattern f tex/generic/hyph-utf8/patterns/ptex/hyph-nb.ec.tex
runpattern f tex/generic/hyph-utf8/patterns/ptex/hyph-nn.ec.tex
runpattern f tex/generic/hyph-utf8/patterns/tex/hyph-nb.tex
runpattern f tex/generic/hyph-utf8/patterns/tex/hyph-nn.tex
runpattern f tex/generic/hyph-utf8/patterns/tex/hyph-no.tex
runpattern f tex/generic/hyph-utf8/patterns/txt/hyph-nb.chr.txt
runpattern f tex/generic/hyph-utf8/patterns/txt/hyph-nb.hyp.txt
runpattern f tex/generic/hyph-utf8/patterns/txt/hyph-nb.lic.txt
runpattern f tex/generic/hyph-utf8/patterns/txt/hyph-nb.pat.txt
runpattern f tex/generic/hyph-utf8/patterns/txt/hyph-nn.chr.txt
runpattern f tex/generic/hyph-utf8/patterns/txt/hyph-nn.hyp.txt
runpattern f tex/generic/hyph-utf8/patterns/txt/hyph-nn.lic.txt
runpattern f tex/generic/hyph-utf8/patterns/txt/hyph-nn.pat.txt
