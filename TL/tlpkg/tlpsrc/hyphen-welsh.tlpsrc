category TLCore
depend hyphen-base
depend hyph-utf8
shortdesc Welsh hyphenation patterns.
longdesc Hyphenation patterns for Welsh in T1/EC and UTF-8 encodings.
execute AddHyphen \
	name=welsh \
	lefthyphenmin=2 \
	righthyphenmin=3 \
	file=loadhyph-cy.tex \
	file_patterns=hyph-cy.pat.txt \
	file_exceptions=
runpattern f texmf-dist/tex/generic/hyph-utf8/loadhyph/loadhyph-cy.tex
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/ptex/hyph-cy.ec.tex
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/tex/hyph-cy.tex
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/txt/hyph-cy.hyp.txt
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/txt/hyph-cy.pat.txt
