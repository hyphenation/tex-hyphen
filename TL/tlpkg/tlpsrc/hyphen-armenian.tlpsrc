category TLCore
depend hyphen-base
depend hyph-utf8
shortdesc Armenian hyphenation patterns.
longdesc Hyphenation patterns for Armenian for Unicode engines.
longdesc Auto-generated from a script included in hyph-utf8.
execute AddHyphen \
	name=armenian \
	lefthyphenmin=1 \
	righthyphenmin=2 \
	file=loadhyph-hy.tex \
	file_patterns=hyph-hy.pat.txt \
	file_exceptions=
srcpattern d texmf-dist/source/generic/hyph-utf8/languages/hy
runpattern f texmf-dist/hyph-utf8/tex/generic/hyph-utf8/patterns/tex-8bit/
runpattern f texmf-dist/tex/generic/hyph-utf8/loadhyph/loadhyph-hy.tex
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/tex/hyph-hy.tex
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/txt/hyph-hy.pat.txt
