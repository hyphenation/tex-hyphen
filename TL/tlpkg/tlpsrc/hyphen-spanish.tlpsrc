category TLCore
depend hyphen-base
depend hyph-utf8
shortdesc Spanish hyphenation patterns.
longdesc Hyphenation patterns for Spanish in T1/EC and UTF-8 encodings.
execute AddHyphen \
	name=spanish synonyms=espanol \
	lefthyphenmin=2 \
	righthyphenmin=2 \
	file=loadhyph-es.tex \
	file_patterns=hyph-es.pat.txt \
	file_exceptions=
docpattern d texmf-dist/doc/generic/hyph-utf8/languages/es
srcpattern d texmf-dist/source/generic/hyph-utf8/languages/es
runpattern f texmf-dist/tex/generic/hyph-utf8/loadhyph/loadhyph-es.tex
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/ptex/hyph-es.ec.tex
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/tex/hyph-es.tex
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/txt/hyph-es.pat.txt
