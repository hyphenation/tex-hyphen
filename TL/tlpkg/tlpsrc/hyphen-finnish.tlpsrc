category TLCore
depend hyphen-base
depend hyph-utf8
shortdesc Finnish hyphenation patterns.
longdesc Hyphenation patterns for Finnish in T1/EC and UTF-8 encodings.
execute AddHyphen \
	name=finnish \
	lefthyphenmin=2 \
	righthyphenmin=2 \
	file=loadhyph-fi.tex \
	file_patterns=hyph-fi.pat.txt \
	file_exceptions=
runpattern f texmf-dist/tex/generic/hyph-utf8/loadhyph/loadhyph-fi.tex
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/ptex/hyph-fi.ec.tex
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/tex/hyph-fi.tex
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/txt/hyph-fi.chr.txt
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/txt/hyph-fi.hyp.txt
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/txt/hyph-fi.lic.txt
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/txt/hyph-fi.pat.txt
