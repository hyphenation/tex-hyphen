category TLCore
depend hyphen-base
depend hyph-utf8
shortdesc Macedonian hyphenation patterns.
longdesc Hyphenation patterns for Macedonian
execute AddHyphen \
	name=macedonian \
	lefthyphenmin=2 \
	righthyphenmin=2 \
	file=loadhyph-mk.tex \
	file_patterns=hyph-mk.pat.txt \
	file_exceptions=
runpattern f texmf-dist/tex/generic/hyph-utf8/loadhyph/loadhyph-mk.tex
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/tex/hyph-mk.tex
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/txt/hyph-mk.chr.txt
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/txt/hyph-mk.hyp.txt
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/txt/hyph-mk.lic.txt
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/txt/hyph-mk.pat.txt
