category TLCore
depend hyphen-base
depend hyph-utf8
shortdesc Interlingua hyphenation patterns.
longdesc Hyphenation patterns for Interlingua in ASCII encoding.
execute AddHyphen \
	name=interlingua \
	lefthyphenmin=2 \
	righthyphenmin=2 \
	file=loadhyph-ia.tex \
	file_patterns=hyph-ia.pat.txt \
	file_exceptions=hyph-ia.hyp.txt
runpattern f texmf-dist/tex/generic/hyph-utf8/loadhyph/loadhyph-ia.tex
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/tex/hyph-ia.tex
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/txt/hyph-ia.chr.txt
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/txt/hyph-ia.hyp.txt
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/txt/hyph-ia.lic.txt
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/txt/hyph-ia.pat.txt
