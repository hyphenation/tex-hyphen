category TLCore
depend hyphen-base
depend hyph-utf8
shortdesc Belarusian hyphenation patterns.
longdesc Belarusian hyphenation patterns in T2A and UTF-8 encodings
execute AddHyphen \
	name=belarusian \
	#lefthyphenmin=2 \
	righthyphenmin=2 \
	file=loadhyph-be.tex \
	file_patterns=hyph-be.pat.txt \
	file_exceptions=
runpattern f texmf-dist/tex/generic/hyph-utf8/loadhyph/loadhyph-be.tex
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/ptex/hyph-be.t2a.tex
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/quote/hyph-quote-be.tex
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/tex/hyph-be.tex
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/txt/hyph-be.chr.txt
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/txt/hyph-be.hyp.txt
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/txt/hyph-be.lic.txt
runpattern f texmf-dist/tex/generic/hyph-utf8/patterns/txt/hyph-be.pat.txt
